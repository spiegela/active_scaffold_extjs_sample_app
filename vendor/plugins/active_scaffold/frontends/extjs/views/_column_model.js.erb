new Ext.grid.ColumnModel([
  <% columns.each do |c| -%>
    {id: "<%= c.name %>", header: "<%= c.label %>", dataIndex: "<%= c.name %>", sortable: "<%= c.sortable? %>"
    <% if c.plural_association? -%>
      , cellActions:[{ iconCls:'nested', qtip:'View <%= c.label %>'}]	
    <% end -%>
    },
  <% end -%>
])